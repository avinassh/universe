{
    "name": "nginx",
    "binary_name": "nginx",
    "priority": 150,
    "environment": "server",
    "long_running": true,
    "docker_arguments": [
        "-p", "9999:9999",
        "-p", "8888:8888",
        "-p", "8009:8009",
        "-p", "8010:8010",
        "-p", "80:80",
        "-p", "443:443",
        "-p", "9001:9001",
        "-p", "50002:50002",
        "-v", "/home/colin/nginx.conf:/etc/nginx/nginx.conf:ro",
        "--mount", "type=bind,source=/home/colin/cert,target=/cert",
        "--init"
    ]
}
