{
    "name": "search_server",
    "binary_name": "search_server",
    "priority": 100,
    "environment": "server",
    "long_running": true,
    "arguments": {
        "web_port": "50001",
        "grpc_port": "50002",
        "index_dir": "/index",
        "auth_hostname": "auth",
        "auth_port": "8888",
        "base_url": "https://search.colinmerkel.xyz"
    },
    "docker_arguments": [
        "--init",
        "--mount", "type=bind,source=/mnt/stateful_partition/index,target=/index"
    ]
}
